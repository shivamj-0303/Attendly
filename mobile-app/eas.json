      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "$EXPO_PUBLIC_API_URL"
      }
    }
  },
  "submit": {
    "production": {}
  }
